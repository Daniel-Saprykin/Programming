// Перепишите программу из преды,дущей врезки "Попробуйте " , выводящую символы и соответствующие им целочисленные 
// значения. с помощью инструкции for. Затем модифицируйте программу так. тобы таблица содержала значения для 
// прописных символов и цифр.

#include <iostream>
#include <clocale>


using namespace std;


int main()
{
	setlocale(LC_ALL, "russian");

	double val1 = 0;
	char currency  = 'a';

	cin >> val1;

	for (int i = 0; i < val1; i++) cout << char('A' + i) << endl;
	
	system("pause");
	return 0;
}
